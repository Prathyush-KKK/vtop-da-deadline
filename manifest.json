{
    "author":"sudonims",
    "manifest_version": 2,
    "name": "Easy DA",
    "version": "0.1.0",
    "description": "Easily see DA",
    "content_scripts": [
        {
            "run_at":"document_start",
            "js": [
                "content.js"
            ],
            "matches": [
                "https://vtop.vit.ac.in/vtop/initialProcess"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "cookies",
        "https://vtop.vit.ac.in/vtop/*"
    ]
}